<!DOCTYPE HTML>
<html>
<head>
  <meta charset="UTF-8">
  <link rel="stylesheet" type="text/css" href="css/menu.css">
  <script src="js/handlebars-v3.0.3.js"></script>
</head>
<body>

<div id="content">
  <h1 class="restaurant-name">{{name_ch}}</h1>
  <ul class="category-list">
    {{#each category_list}}
    <li class="category">
      <div class="category-name">
        <div class="category-name-ch">{{this.name_ch}}</div>
        <div class="category-name-en">{{this.name_en}}</div>
      </div>
      <ul class="item-list">
        {{#each this.item_list}}
        <li class="item">
          <div class="item-name">
            <div class="item-name-ch">{{this.name_ch}}</div>
            <div class="item-name-en">{{this.name_en}}</div>
          </div>
          <div class="item-price">{{this.price}}</div>
        </li>
        {{/each}}
      </ul>
    {{/each}}
  </ul>
</div>

<script>
  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
  m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
  })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

  ga('create', 'UA-43129579-2', 'auto');
  ga('send', 'pageview');
</script>

</body>
</html>
